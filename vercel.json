{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/server.ts"
        }
    ],
    "crons": [
        {
            "path": "/jobs/process_recurring_entries",
            "schedule": "0 * * * *"
        }
    ]
}