{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/server.ts"
        }
    ],
    "redirects": [
        {
            "source": "/:path*",
            "destination": "https://contai-api.vercel.app/:path*",
            "statusCode": 308
        }
    ],
    "crons": [
        {
            "path": "/jobs/process_recurring_entries",
            "schedule": "0 0 * * *"
        }
    ]
}